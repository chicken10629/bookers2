
<div class="container">
  <div class="row">
    <%= render 'list', user: @user_temp , book: @book_temp %>
    <div class="col-md-8 offset-md-1 mt-3">
      <h2>
        Users
      </h2>
      <table class="table">
        <tr>
          <th>
            image
          </th>
          <th>
            name
          </th>
          <th>
          </th>
        </tr>
        <% @user_all.each do |user| %>
          <tr>
            <td>
              <%= image_tag user.get_profile_image %>
            </td>
            <td>
              <%= user.name %>
            </td>
            <td>
              <%= link_to "Show", user_path(user.id) %>
            </td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>