
<div class="container">
  <div class="row">
    <%= render 'list', user: @user_temp , book: @book_temp %>
    <div class="col-md-8 offset-md-1">
      <h1>
        Users
      </h1>
      <table>
        <tr>
          <th>
            image
          </th>
          <th>
            name
          </th>
          <th>
          </th>
        </tr>
        <% @user_all.each do |user| %>
          <tr>
            <td>
              <%= image_tag user.get_profile_image %>
            </td>
            <td>
              <%= user.name %>
            </td>
            <td>
              <%= link_to "show", user_path(user.id) %>
            </td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>